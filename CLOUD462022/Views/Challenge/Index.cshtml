@model List<Blob>

<div class="p-4 container">
    <div class="row">
        <h4 class="text-info">Challenge</h4>
    </div>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-4 border p-2">
                <h4 class="text-success">Guess this plant!</h4>
                <img src="@item.Uri" width="200" />
            </div>}
    </div>
</div>